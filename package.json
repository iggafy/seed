{
  "name": "seed",
  "private": true,
  "main": "electron/main.js",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "electron .",
    "electron:dev": "concurrently -k \"cross-env NODE_ENV=development vite\" \"wait-on tcp:3000 && cross-env NODE_ENV=development electron .\"",
    "dist": "npm run build && electron-builder"
  },
  "description": "Shared Exploration & Emergent Discovery Tool",
  "author": "SEED Team <seed@example.com>",
  "build": {
    "appId": "com.seed.app",
    "productName": "SEED",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json"
    ],
    "linux": {
      "target": [
        "deb"
      ],
      "category": "Education"
    }
  },
  "dependencies": {
    "@google/genai": "^1.34.0",
    "d3": "^7.9.0",
    "lucide-react": "^0.562.0",
    "openai": "^6.15.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "wait-on": "^9.0.3"
  }
}